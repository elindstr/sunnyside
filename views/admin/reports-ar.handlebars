<h1>Aged Receivables</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>0-30</th>
      <th>31-60</th>
      <th>61-90</th>
      <th>91+</th>
      <th>Total</th>
    </tr>
  </thead>
  <tbody>
    <tr>
    {{#each arObj}}
    <tr>
      <td><a href="/admin/customers/view/{{this.id}}">{{this.first_name}}  {{this.last_name}}</a></td>
      <td>{{this.ar0}}</td>
      <td>{{this.ar30}}</td>
      <td>{{this.ar60}}</td>
      <td>{{this.ar90}}</td>
      <td>{{this.arTotal}}</td>
    </tr>
    {{/each}}
    
    <td><b>Totals</b></td>
      <td>{{ar0Total}}</td>
      <td>{{ar30Total}}</td>
      <td>{{ar60Total}}</td>
      <td>{{ar90Total}}</td>
      <td>{{arTotal}}</td>
    </tr>
  </tbody>
</table>
