<h1>Sunnyside Pools</h1>
<br>    
<h2>Welcome {{userData.first_name}}</h2>
<div>
    <h3>My Information</h3>
    <table id="employees">
        <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Role</th>
            <th>Available Days</th>
        </tr>
        </thead>
        <tbody>
            <!-- data inserted here -->
            {{#each employees}}
        <tr>
            <td>{{this.first_name}}</td>
            <td>{{this.last_name}}</td>
            <td>{{this.email}}</td>
            <td>{{this.role}}</td>
            <td>{{this.available_days}}</td>
        </tr>
        {{/each}}
        </tbody>
    </table>
</div>