<h1>{{customer.first_name}}'s Information</h1>

<table>
  <tr>
    <td><span>First Name</span></td>
    <td><input class="edit-inputs" type="text" id="first_name" value="{{customer.first_name}}"/></td>
  </tr>
  <tr>
    <td><span>Last Name</span></td>
    <td><input class="edit-inputs" type="text" id="last_name" value="{{customer.last_name}}" /></td>
  </tr>
  <tr>
    <td><span>Address</span></td>
    <td><input class="edit-inputs" type="text" id="address" value="{{customer.address}}" /></td>
  </tr>
  <tr>
    <td><span>Phone</span></td>
    <td><input class="edit-inputs" type="text" id="phone" value="{{customer.phone}}" /></td>
  </tr>
  <tr>
    <td><span>Email</span></td>
    <td><input class="edit-inputs" type="text" id="email" value="{{customer.email}}" /></td>
  </tr>
</table>
<button id="update" onclick="updateCustomer()">Update</button>
<a href="/dashboard"><button id="cancel">Cancel</button></a>

<script src="/js/admin-customers.js"></script>